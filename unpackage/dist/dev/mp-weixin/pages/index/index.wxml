<view class="center"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['list','',index]]]]]]]}}" class="swiper-item" bindtap="__e"><image class="swiper-item-img" src="{{item.img_src}}"></image></view></swiper-item></block></swiper><view class="uni-grid-12 uni-common-mt"><block wx:for="{{grids}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-grid-12-item" hover-class="uni-grid-12-item-hover" data-event-opts="{{[['tap',[['tapIcon',['$0'],[[['grids','',index]]]]]]]}}" bindtap="__e"><image class="uni-grid-12-image tag-img img-backgroud" src="{{item.xinimg}}" lazy-load="true" data-event-opts="{{[['error',[['showDefImg',[index]]]]]}}" binderror="__e"></image><text class="uni-grid-12-text icon-text">{{item.title+"["+item.Number+"]"}}</text></view></block></view></view>